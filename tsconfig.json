{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "node",
      "cypress"
    ],
    "typeRoots": [
      "node_modules/@types",
      "node_modules/cypress/types",
      "node_modules/cypress-image-diff-js/types"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@cy/*": [
        "./cypress/*"
      ],
      "@e2e/*": [
        "./cypress/e2e/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "cypress/**/*.ts",
    "cypress.config.ts",
    "./*.config.ts"
  ]
}
